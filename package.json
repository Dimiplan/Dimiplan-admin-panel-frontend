{
  "name": "admin-panel",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "css": "tailwindcss --input src/tailwind.css --output src/tailwind-output.css",
    "start": "bun run css && ng serve",
    "build": "bun run css && ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint \"src/**/*.{ts,html,json,md,yml}\" && prettier --check .",
    "lint:fix": "eslint \"src/**/*.{ts,html,json,md,yml}\" --fix && prettier -w ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.2.1",
    "@angular/cdk": "20.2.0",
    "@angular/common": "20.2.1",
    "@angular/compiler": "20.2.1",
    "@angular/core": "20.2.1",
    "@angular/forms": "20.2.1",
    "@angular/material": "20.2.0",
    "@angular/platform-browser": "20.2.1",
    "@angular/router": "20.2.1",
    "@tailwindcss/cli": "^4.1.12",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1",
    "sanitize-html": "^2.17.0"
  },
  "devDependencies": {
    "@angular/build": "20.2.0",
    "@angular/cli": "20.2.0",
    "@angular/compiler-cli": "20.2.1",
    "@biomejs/biome": "2.2.2",
    "@types/jasmine": "5.1.9",
    "@types/sanitize-html": "^2.16.0",
    "jasmine-core": "5.9.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "postcss-scss": "^4.0.9",
    "tailwindcss": "^4.1.12",
    "typescript": "5.9.2",
    "wrangler": "^4.33.0"
  }
}
